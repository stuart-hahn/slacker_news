<%= form_with model: @link, local: true do |f| %>
<%= render '/shared/errors', object: @link %>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :url %>
    <%= f.url_field :url %>

    <%= f.label :category_name %>
    <%= f.text_field :category_name %>

    <%= f.submit %>
<% end %>